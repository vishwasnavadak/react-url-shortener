{
  "name": "react-url-shortener",
  "version": "1.0.0",
  "description": "Serverless url-shortener using React.js and Firebase.",
  "main": "webpack.conf.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "bundle": "node_modules/.bin/webpack --config webpack.config.js",
    "lint": "node_modules/.bin/eslint src/**.js",
    "test": "node_modules/.bin/karma test/karma.conf.js",
    "build": "npm run lint && npm run bundle && npm run test",
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --hot --port 3000 --host 0.0.0.0",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --cache --host 0.0.0.0 --port 3000 --public react-url-shortener.firebase.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neetjn/react-url-shortener.git"
  },
  "keywords": [
    "react",
    "javascript",
    "js",
    "firebase",
    "url-shortener",
    "bulma"
  ],
  "author": "John Nolette <john@neetgroup.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neetjn/react-url-shortener/issues"
  },
  "homepage": "https://github.com/neetjn/react-url-shortener#readme",
  "devDependencies": {
    "animate.css": "^3.6.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bulma": "^0.6.2",
    "bulmaswatch": "^0.6.1",
    "css-loader": "^0.28.9",
    "d3": "^4.12.2",
    "dotenv-webpack": "^1.5.4",
    "electron": "^1.7.11",
    "eslint": "^4.14.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "firebase": "^4.9.1",
    "foundation-icons": "^1.0.1",
    "google-fonts-webpack-plugin": "^0.4.3",
    "karma": "^2.0.0",
    "karma-electron": "^5.2.2",
    "node-sass": "4.0.0",
    "raw-loader": "0.5.1",
    "react": "^16.2.0",
    "react-anime": "^2.0.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.0",
    "url-loader": "0.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  }
}
